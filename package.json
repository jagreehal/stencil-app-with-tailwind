{
  "name": "stencil-app-with-tailwind",
  "private": true,
  "version": "0.0.1",
  "description": "Creating Stencil Apps with Tailwind",
  "scripts": {
    "build": "stencil build",
    "build:local": "BASE_URL=http://localhost:5000/ stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.5.2",
    "@stencil/postcss": "^2.0.0",
    "@stencil/router": "^1.0.1",
    "@types/node": "^14.14.37",
    "autoprefixer": "^10.2.5",
    "tailwindcss": "^2.0.2"
  },
  "license": "MIT"
}
